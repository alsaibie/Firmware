uorb start
param load
dataman start
param set BAT_N_CELLS 2
param set MAV_TYPE 3
param set SYS_AUTOSTART 60001
param set SYS_RESTART_TYPE 2
param set COM_DISARM_LAND 0

param set DP_ROLLRATE_P 0.2
param set DP_PITCHRATE_P 0.2
param set DP_PITCH_P 6
param set DP_ROLL_P 6

# What are those?
param set CAL_GYRO0_ID 2293768
param set CAL_ACC0_ID 1376264
param set CAL_ACC1_ID 1310728
param set CAL_MAG0_ID 196616

simulator start -s

# I don't need pwm output, I just need a ROS node to connect the mixed_att_control output

pwm_out_sim mode_pwm
mixer load /dev/pwm_output0 ROMFS/px4fmu_test/mixers/IO_pass.mix

gyrosim start
accelsim start
barosim start
adcsim start

# What is this?
sleep 1

# Sensors includes the transmitter/rc so I need it
sensors start

# I need the commander to arm and such.
commander start

# Attitude estimator yes I need it.
attitude_estimator_q start

# Start att control
dp_att_control start

# Start Local position estimator
# local_position_estimator start

# Start pos control
#dp_pos_control start


# Setup the mavlink environment
mavlink start -u 14556 -r 4000000
mavlink start -u 14557 -r 4000000 -m onboard -o 14540
mavlink stream -r 50 -s LOCAL_POSITION_NED -u 14556
mavlink stream -r 50 -s SERVO_OUTPUT_RAW_0 -u 14556
mavlink stream -r 50 -s ATTITUDE -u 14556
mavlink stream -r 50 -s ATTITUDE_QUATERNION -u 14556
mavlink stream -r 50 -s ATTITUDE_TARGET -u 14556
mavlink stream -r 50 -s SERVO_OUTPUT_RAW_0 -u 14556
mavlink stream -r 20 -s RC_CHANNELS -u 14556
mavlink stream -r 250 -s HIGHRES_IMU -u 14556
logger start -r 100 -e
mavlink boot_complete
