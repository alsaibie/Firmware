#!nsh
#
# Dolphin Default Configuration. Gets called by 60001_uAUV_Dolphin
#
# Ali AlSaibie <ali@alsaibie.com>
#

set VEHICLE_TYPE uAUV

if [ $AUTOCNF == yes ]
then
	param set PE_VELNE_NOISE 0.5
	param set PE_VELD_NOISE 0.35
	param set PE_POSNE_NOISE 0.5
	param set PE_POSD_NOISE 1.25

    param set DP_ROLL_P         0.500
    param set DP_ROLL_FF        0.001
    param set DP_ROLLRATE_P     0.010
    param set DP_ROLLRATE_I     0.001
    param set DP_ROLLRATE_D     0.001
    param set DP_ROLLRATE_FF    0.001
    param set DP_ROLLRATE_MAX   80.000
    param set DP_RR_INT_LIM     0.200

    param set DP_PITCH_P        1.000
    param set DP_PITCHRATE_P    0.030
    param set DP_PITCHRATE_I    0.050
    param set DP_PITCHRATE_D    0.0001
    param set DP_PITCHRATE_FF   0.075
    param set DP_PITCHRATE_MAX  100.000
    param set DP_PR_INT_LIM     0.075

    param set DP_YAW_P          0.030
    param set DP_YAWRATE_P      0.030
    param set DP_YAWRATE_I      0.050
    param set DP_YAWRATE_D      0.0001
    param set DP_YAWRATE_FF     0.075
    param set DP_YAWRATE_MAX    120.000
    param set DP_YAWRATE_FF     0.075
    param set DP_YR_INT_LIM     0.750

    param set DP_ACRO_R_MAX 360.0
    param set DP_ACRO_P_MAX 360.0

    param set DPC_MAX_ROLL      120.000
    param set DPC_MAX_TILT      120.000

    param set DPC_THR_MIN 0.06
    param set DPC_MANTHR_MIN 0.06

    param set DP_ROTOR_R 0.03

    # PWM range
    param set PWM_DISARMED 900
    param set PWM_MIN 1100
    param set PWM_MAX 1900

    param set SENS_BOARD_ROT 0
    param set SENS_BOARD_X_OFF 0.000001
    param set SYS_AUTOSTART 60001

fi

set PWM_RATE 400
set PWM_OUT 1234


